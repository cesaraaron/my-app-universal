{
  "name": "my-app-universal",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/expo": "^27.0.3",
    "@types/expo__vector-icons": "^6.2.2",
    "@types/ip": "^0.0.31",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.110",
    "@types/react": "^16.3.14",
    "@types/react-native": "^0.55.14",
    "@types/react-navigation": "^2.0.7",
    "@types/react-test-renderer": "^16.0.1",
    "apollo-codegen": "^0.20.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-dotenv": "^0.1.1",
    "babel-plugin-expo-web": "^0.0.5",
    "babel-plugin-react-native-web": "^0.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "concurrently": "^3.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.7",
    "electron": "^2.0.3",
    "file-loader": "^1.1.7",
    "husky": "^0.14.3",
    "jest-expo": "~27.0.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.6",
    "react-native-scripts": "latest",
    "react-native-scripts-ts": "1.15.0",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "16.3.1",
    "style-loader": "^0.19.0",
    "ts-jest": "^22.4.6",
    "ts-loader": "3.5.0",
    "tslib": "^1.9.1",
    "typescript": "2.9.2",
    "wait-on": "^2.1.0"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry-web.js",
  "scripts": {
    "start": "node scripts/updateEndpoints && react-native-scripts-ts start",
    "eject": "react-native-scripts-ts eject",
    "android": "react-native-scripts-ts android",
    "ios": "react-native-scripts-ts ios",
    "test": "jest",
    "web": "node scripts/updateEndpoints && webpack-dev-server -d --config ./webpack.config.js  --inline --hot --colors --content-base public/ --history-api-fallback",
    "electron": "cross-env NODE_ENV=development electron public/electron",
    "dev": "concurrently 'npm run web' 'wait-on http://localhost:8080/ && npm run electron'",
    "build": "NODE_ENV=production webpack -p --config ./webpack.config.js",
    "generate": "apollo-codegen introspect-schema http://localhost:4000 --output src/schema.json && apollo-codegen generate ./src/queries.ts --add-typename --schema ./src/schema.json --target typescript --output ./src/__generated__/types.ts",
    "precommit": "lint-staged"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "antd-mobile": "^2.2.0",
    "antd-mobile-rn": "^2.2.0",
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.3.5",
    "apollo-link": "^1.2.2",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.4",
    "apollo-link-queue": "^2.0.1",
    "apollo-link-retry": "^2.2.4",
    "apollo-link-state": "^0.4.1",
    "apollo-link-ws": "^1.0.8",
    "expo": "^27.0.1",
    "expo-web": "^0.0.12",
    "formik": "^0.11.11",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "ip": "^1.1.5",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "native-base": "^2.6.1",
    "react": "16.3.1",
    "react-apollo": "^2.1.6",
    "react-dom": "16.3.1",
    "react-native": "~0.55.2",
    "react-native-dotenv": "^0.1.1",
    "react-native-modal": "^6.1.0",
    "react-native-web": "^0.4.0",
    "react-navigation": "1.5.11",
    "subscriptions-transport-ws": "^0.9.11",
    "webpack": "^3.11.0",
    "webpack-dev-server": "2.9.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
